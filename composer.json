{
    "name": "tapday/notification",
    "description": "This package is to handle Notifications of Shop on Discord.",
    "type": "library",
    "keywords": ["laravel", "discord", "notifications", "webhook"],
    "license": "MIT",
    "authors": [
        {
            "name": "noman hameed",
            "email": "nomanbutt8322@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Tapday\\Notifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tapday\\Notifications\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tapday\\Notifications\\DiscordNotificationsServiceProvider"
            ],
            "aliases": {
                "Discord": "Tapday\\Notifications\\Facades\\Discord"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
